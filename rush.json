{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.20.0",
  "yarnVersion": "1.22.1",
  "projectFolderMaxDepth": 3,
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "projects": [
    {
      "packageName": "@decorize/core-dev",
      "projectFolder": "package/core",
      "versionPolicyName": "prerelease",
      "shouldPublish": false
    },
    {
      "packageName": "@decorize/core",
      "projectFolder": "package/core/dist",
      "versionPolicyName": "prerelease",
      "shouldPublish": true
    },
    {
      "packageName": "@decorize/bind-dev",
      "projectFolder": "package/bind",
      "versionPolicyName": "prerelease",
      "shouldPublish": false
    },
    {
      "packageName": "@decorize/bind",
      "projectFolder": "package/bind/dist",
      "versionPolicyName": "prerelease",
      "shouldPublish": true
    },
    {
      "packageName": "@decorize/cache-dev",
      "projectFolder": "package/cache",
      "versionPolicyName": "prerelease",
      "shouldPublish": false
    },
    {
      "packageName": "@decorize/cache",
      "projectFolder": "package/cache/dist",
      "versionPolicyName": "prerelease",
      "shouldPublish": true
    },
    {
      "packageName": "@decorize/cli-dev",
      "projectFolder": "package/cli",
      "versionPolicyName": "prerelease",
      "shouldPublish": false
    },
    {
      "packageName": "@decorize/cli",
      "projectFolder": "package/cli/dist",
      "versionPolicyName": "prerelease",
      "shouldPublish": true
    },
    {
      "packageName": "@decorize/entity-dev",
      "projectFolder": "package/entity",
      "versionPolicyName": "prerelease",
      "shouldPublish": false
    },
    {
      "packageName": "@decorize/entity",
      "projectFolder": "package/entity/dist",
      "versionPolicyName": "prerelease",
      "shouldPublish": true
    }
  ],
  "gitPolicy": {
    "versionBumpCommitMessage": "Publish package"
  }
}
