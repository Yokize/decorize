{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.23.4",
  "yarnVersion": "1.22.4",
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "repository": {
    "url": "https://github.com/Yokize/decorize.git"
  },
  "projects": [
    {
      "packageName": "@decorize/core",
      "projectFolder": "package/core",
      "versionPolicyName": "individual"
    },
    {
      "packageName": "@decorize/bind",
      "projectFolder": "package/bind",
      "versionPolicyName": "individual"
    },
    {
      "packageName": "@decorize/cache",
      "projectFolder": "package/cache",
      "versionPolicyName": "individual"
    }
  ]
}
