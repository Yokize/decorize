# Opinionated code formatter which removes all original styling and ensures
# that all outputted code conforms to a consistent style by reprinting the
# parsed AST with its own rules.
# Â© Prettier <https://prettier.io>

# LF at the end of line.
endOfLine: lf

# Line length used to wrap on.
printWidth: 120

# Single quotes for string literal.
singleQuote: true

# No trailing commas on multi-line.
trailingComma: none
